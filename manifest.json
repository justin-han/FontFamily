{
  "manifest_version": 3,
  "name": "Font Changer",
  "version": "1.0.0",
  "description": "An extension that allows the user to change the font style and size.",
  "browser_action":{
      "default_popup": "popup.html"
  },
  "permissions":["activeTab", "<all_urls>"],
  "content_scripts":[
      {
          "matches":["<all_urls>"],
          "js":["popup.js"]

      } 
 ]
}